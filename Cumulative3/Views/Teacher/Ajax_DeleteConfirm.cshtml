@model Cumulative3.Models.Teacher
@{
    ViewBag.Title = "Ajax_DeleteConfirm";
}

<div id="containershow">
    <nav>
        <a class="navlink" href="/Teacher/Show/@Model.TeacherId">Cancel</a>
    </nav>
    <h2>Are you sure you want to delete @Model.TeacherFname @Model.TeacherLname?</h2>
    <p>This action cannot be undone.</p>
    <button class="delbutton" onclick="DeleteTeacher(@Model.TeacherId)">Confirm Delete</button>
</div>

